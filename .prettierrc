{
  "singleQuote": true,
  "bracketSpacing": true,
  "semi": false,
  "tabWidth": 2,
  "printWidth": 200,
  "trailingComma": "none",
  "bracketSameLine": true,
  "jsxBracketSameLine": false,
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "html"
      }
    }
  ]
}
